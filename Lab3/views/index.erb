<div class="grid-x grid-padding-x">
    <div class="large-12 cell">
        <table>
        <thead>
            <tr>
            <th>Дата прочтения</th>
            <th>Автора "Название"</th>
            </tr>
        </thead>
        <tbody>
            <% @all_books.each do |book| %>
            <tr>
                <td><%= book.data %></td>
                <td><%= "#{book.title} '#{book.author}'" %></td>
            </tr>
            <% end %>
        </tbody>
        </table>
    </div>
</div>